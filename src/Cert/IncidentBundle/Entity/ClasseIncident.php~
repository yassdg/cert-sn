<?php

namespace Cert\IncidentBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ClasseIncident
 */
class ClasseIncident
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var array
     */
    private $groupe;

    /**
     * @var array
     */
    private $severite;

    /**
     * @var integer
     */
    private $tempDeReponse;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set groupe
     *
     * @param array $groupe
     * @return ClasseIncident
     */
    public function setGroupe($groupe)
    {
        $this->groupe = $groupe;

        return $this;
    }

    /**
     * Get groupe
     *
     * @return array 
     */
    public function getGroupe()
    {
        return $this->groupe;
    }

    /**
     * Set severite
     *
     * @param array $severite
     * @return ClasseIncident
     */
    public function setSeverite($severite)
    {
        $this->severite = $severite;

        return $this;
    }

    /**
     * Get severite
     *
     * @return array 
     */
    public function getSeverite()
    {
        return $this->severite;
    }

    /**
     * Set tempDeReponse
     *
     * @param integer $tempDeReponse
     * @return ClasseIncident
     */
    public function setTempDeReponse($tempDeReponse)
    {
        $this->tempDeReponse = $tempDeReponse;

        return $this;
    }

    /**
     * Get tempDeReponse
     *
     * @return integer 
     */
    public function getTempDeReponse()
    {
        return $this->tempDeReponse;
    }
}
